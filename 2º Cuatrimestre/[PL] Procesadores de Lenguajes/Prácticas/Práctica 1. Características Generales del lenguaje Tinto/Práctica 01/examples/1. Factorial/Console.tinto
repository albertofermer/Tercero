
/**
 * Conjunto de funciones para acceder a la consola
 */
native Console {
  
  /**
   * Escribe un carácter en la consola
   */
  public void print(char c);

  /**
   * Escribe un número entero en la consola
   */
  public void print(int i);

  /**
   * Lee un número entero de la consola
   */  
  public int readInt();

  /**
   * Lee un carácter de la consola
   */
  public char readChar();
 
}